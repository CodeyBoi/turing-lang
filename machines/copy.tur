# copies the current symbol and moves it to the left
states init q0 q1 q_ qD q& q| q! q^
syms 0 1 _ D & | ! ^
initstate init
finalstates HALT
table
init 0 q0 . L
q0 * HALT 0 R
init 1 q1 . L
q1 * HALT 1 R
init _ q_ . L
q_ * HALT _ R
init D qD . L
qD * HALT D R
init & q& . L
q& * HALT & R
init | q| . L
q| * HALT | R
init ! q! . L
q! * HALT ! R
init ^ q^ . L
q^ * HALT ^ R
